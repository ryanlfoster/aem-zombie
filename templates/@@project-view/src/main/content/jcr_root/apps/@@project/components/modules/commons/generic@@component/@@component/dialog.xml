<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          stateful="false"
          xtype="dialog"
          height="600"
          width="800">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <collection
                        jcr:primaryType="cq:Widget"
                        path="/apps/@@project/components/modules/commons/genericslider/GenericSlider/collection-config.infinity.json"                        
                        xtype="cqinclude"/>
                 <assetselector
                        jcr:primaryType="cq:Widget"
                        path="/apps/@@project/components/modules/commons/genericslider/HeroCarousel/selector-assets.infinity.json"
                        xtype="cqinclude"/>
                 <storyquery
                    jcr:primaryType="cq:Widget"
                    path="/apps/natgeo/components/modules/util/query/query.infinity.json"
                    xtype="cqinclude"/>
            </items>
        </tabs>
    </items>
</jcr:root>